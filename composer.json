{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "alibabacloud/client": "^1.5",
        "alibabacloud/sdk": "^1.8",
        "aliyuncs/oss-sdk-php": "^2.4",
        "flipbox/lumen-generator": "^8.2",
        "godruoyi/php-snowflake": "^1.0",
        "google/protobuf": "^3.14",
        "hashids/hashids": "^4.1",
        "illuminate/filesystem": "^8.26",
        "illuminate/mail": "^8.23",
        "illuminate/redis": "^8.23",
        "jenssegers/agent": "^2.6",
        "laravel/lumen-framework": "^8.0",
        "maatwebsite/excel": "^3.1",
        "monolog/monolog": "^2.2",
        "overtrue/laravel-lang": "~5.0",
        "predis/predis": "^1.1",
        "riverslei/payment": "*",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "brainmaestro/composer-git-hooks": "^2.8",
        "fakerphp/faker": "^1.9.1",
        "league/flysystem": "^1.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "GPBMetadata\\": "protobuf/compile/GPBMetadata"
        },
        "files": [
            "app/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "protobuf/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "lint": "phpcs --standard=PSR1,./ruleset.xml app/Http bootstrap/ config/ database/ routes/",
        "lint-fix": "phpcbf --standard=PSR1,PSR2 app/Http bootstrap/ config/ database/ routes/"
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "echo committing as $(git config user.name).",
                "composer run lint"
            ]
        }
    }
}
